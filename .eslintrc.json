{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "object-curly-newline": "off",
    "import/extensions": "off",
    "comma-dangle": "off",
    "max-classes-per-file": "off"
  }
}
